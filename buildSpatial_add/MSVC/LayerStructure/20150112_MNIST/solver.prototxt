#net: "train_val.prototxt"
net: "train_val_Mnist.prototxt"
#net: "train_val_reconstruct.prototxt"
test_iter: 100
test_interval: 500
test_initialization: false

base_lr: 0.001
lr_policy: "step"
gamma: 1
stepsize: 50000
display: 500
max_iter: 300000
momentum: 0.9
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "snapshot_mnist/MNIST"
solver_mode: GPU
