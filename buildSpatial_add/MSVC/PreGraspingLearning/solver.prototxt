net: "PreGraspingLearning/train_val_pregrasping_bn.prototxt"

test_iter: 10
test_interval: 1000
test_initialization: false

base_lr: 0.0001
#base_lr: 0.00001
lr_policy: "inv"
gamma: 0.0001
power: 0.75
display: 10
max_iter: 600000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
#snapshot_prefix: "PreGraspingLearning/snapshot_pregrasping/End_to_End"
snapshot_prefix: "PreGraspingLearning/snapshot_pregrasping_linearCheck/End_to_End"
solver_mode: GPU
