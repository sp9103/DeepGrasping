net: "PreGraspingLearning/train_val_pregrasping_cnn_bn.prototxt"

test_iter: 10
test_interval: 1000
test_initialization: true

base_lr: 0.01
lr_policy: "inv"
gamma: 0.0001
power: 0.75
display: 10
